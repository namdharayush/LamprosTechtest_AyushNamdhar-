def shuffleOne(nums, n):
    result = [0] * (n * 2)  #Initialize the en empty result arrayof size n*2 
    for i in range(n):   #Loop through the input array from index 0 to n-1
        result[2*i] = nums[i]   #For each iteration, add nums[i] to the result array at index 2*i.
        result[2*i+1] = nums[i+n]  #For each iteration, add nums[i+n] to the result array at index 2*i+1
    return result  #return the result an array.
nums = [2,5,1,3,4,7]
n=3
print("This Output is Example 1 : ",shuffleOne(nums,n))

def shuffleTwo(nums, n):
    result = [0] * (n * 2)   #Initialize the en empty result arrayof size n*2 
    for i in range(n):    #Loop through the input array from index 0 to n-1
        result[2*i] = nums[i]   #For each iteration, add nums[i] to the result array at index 2*i.
        result[2*i+1] = nums[i+n]   #For each iteration, add nums[i+n] to the result array at index 2*i+1
    return result   #return the result an array.
nums = [1,2,3,4,4,3,2,1]
n=4
print("This Output is Example 2 : ",shuffleTwo(nums,n))


def shuffleThree(nums, n):
    result = [0] * (n * 2)   #Initialize the en empty result arrayof size n*2 
    for i in range(n):    #Loop through the input array from index 0 to n-1
        result[2*i] = nums[i]   #For each iteration, add nums[i] to the result array at index 2*i.
        result[2*i+1] = nums[i+n]   #For each iteration, add nums[i+n] to the result array at index 2*i+1
    return result   #return the result an array.
nums = [1,1,2,2]
n=2
print("This Output is Example 3 : ",shuffleThree(nums,n))
 
